        "prettier-stylelint": "https://github.com/Graffino/prettier-stylelint",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.32.8",
        "sass-loader": "^11.0.1",
        "stylelint": "^13.12.0",
