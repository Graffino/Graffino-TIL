{
    "name": "graffino/til",
    "version": "1.0.19",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "andreasindal/laravel-markdown": "^2.0",
        "fideloper/proxy": "~4.0",
        "laravel-notification-channels/twitter": "^1.0",
        "laravel/framework": "^5.7",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "predis/predis": "^1.1",
        "pusher/pusher-php-server": "~3.0",
        "frenzy/turbolinks": "~3.2.0",
        "nesbot/carbon": "1.39.1 as 2.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "composer/composer": "^2.0",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "laravel/dusk": "^5.11",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
